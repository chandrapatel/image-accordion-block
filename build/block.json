{
  "$schema": "https://schemas.wp.org/trunk/block.json",
  "apiVersion": 2,
  "name": "cpb/image-accordion-block",
  "version": "0.1.0",
  "title": "Image Accordion Block",
  "category": "media",
  "icon": "images-alt",
  "description": "Image Accordion Block",
  "attributes": {
    "headerText": {
      "type": "string",
      "source": "html",
      "selector": "div.accordion-header h3"
    },
    "accordion": {
      "type": "number",
      "default": 2
    },
    "accordionContent": {
      "type": "array",
      "default": [],
      "source": "query",
      "selector": ".accordion-wrapper",
      "query": {
        "title": {
          "type": "string",
          "source": "html",
          "selector": ".accordion-title",
          "default": ""
        },
        "content": {
          "type": "string",
          "source": "html",
          "selector": ".accordion-content",
          "default": ""
        }
      }
    },
    "accordionImages": {
      "type": "array",
      "default": [],
      "source": "query",
      "selector": ".accordion-images img",
      "query": {
        "url": {
          "type": "string",
          "source": "attribute",
          "attribute": "src"
        },
        "alt": {
          "type": "string",
          "source": "attribute",
          "attribute": "alt",
          "default": ""
        },
        "id": {
          "type": "string",
          "source": "attribute",
          "attribute": "data-id"
        }
      }
    },
    "style": {
      "type": "object",
      "default": {
        "color": {
          "background": "#E1F1DE",
          "text": "#3C4043"
        }
      }
    }
  },
  "supports": {
    "color": {
      "text": true,
      "background": true
    },
    "html": false
  },
  "textdomain": "image-accordion-block",
  "editorScript": "file:./index.js",
  "editorStyle": "file:./index.css",
  "style": "file:./style-index.css"
}